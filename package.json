{
    "name": "yuck",
    "displayName": "yuck",
    "description": "Yuck language support for Eww Widgets",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "yuck",
            "aliases": ["yuck", "yuck"],
